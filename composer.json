{
    "name": "olssonm/loopia-api",
    "description": "Simple interface for the Loopia XMLRPC-API",
    "keywords": [
        "olssonm",
        "Loopia",
        "XML-RPC"
    ],
    "homepage": "https://github.com/olssonm/loopia-api",
    "license": "MIT",
    "require": {
        "php": "^7.0|^8.0",
        "lstrojny/fxmlrpc": "^0.20",
        "zendframework/zend-diactoros": "^1.3",
        "php-http/message": "^1.11",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.5"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\LoopiaApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\LoopiaApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
